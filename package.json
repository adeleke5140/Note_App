    "babel-loader": "^8.2.4",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-react": "6.24.1",
